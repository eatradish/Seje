<article class="post">
    <div class="post-title">
        <h1 class="title"><%= page.title %></h2>
    </div>
    <div class="post-content">
        <%- page.content %>
    </div>
    <div class="post-meta">
        <span class="post-time"><%- date(page.date, "YYYY.MM.DD") %></span>
    </div>
</article>

<div class="prev_next">
    <%- partial('_partial/prev_next') %>
</div>

<div class="post-comment">
    <%- partial('_partial/disqus') %>
</div>